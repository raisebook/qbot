---
family: <%= family('qbot') %>
taskRoleArn: <%= ecs_task_role('qbot') %>
containerDefinitions:
- name: qbot
  image: raisebook/qbot:<%= docker_image_tag %>
  essential: true
  cpu: 128
  memory: 128
  logConfiguration:
    logDriver: syslog
    options:
      syslog-address: udp://logs5.papertrailapp.com:26166
